  end
  object q_get_alarm_only_hcbu: TFDQuery
    Connection = PRINCIPALE.cn1
    SQL.Strings = (
      
        'SELECT MARCHE, GTOT_MIN, GTOT_MAX, IAS_MAX, VSI_MAX, PITCH_MAX, ' +
        'ROLL_MAX, HEIGHT_MIN, ALTITUDE_MAX'
      'FROM ETL.BOX_ALARM')
    Left = 67
    Top = 202
  end
  object q_update_alarm: TFDQuery
    Connection = PRINCIPALE.cn1
    SQL.Strings = (
      'UPDATE ETL.BOX_ALARM'
      'SET'
      '    GTOT_MIN = :gtot_min,'
      '    GTOT_MAX = :gtot_max,'
      '    IAS_MAX = :ias_max,'
      '    VSI_MAX = :vsi_max,'
      '    PITCH_MAX = :pitch_max,'
      '    ROLL_MAX = :roll_max,'
      '    HEIGHT_MIN = :height_min,'
      '    ALTITUDE_MAX = :altitude_max'
      'WHERE'
      '    MARCHE = :marche;'
      '')
    Left = 171
    Top = 202
    ParamData = <
      item
        Name = 'GTOT_MIN'
        ParamType = ptInput
      end
      item
        Name = 'GTOT_MAX'
        ParamType = ptInput
      end
      item
        Name = 'IAS_MAX'
        ParamType = ptInput
      end
      item
        Name = 'VSI_MAX'
        ParamType = ptInput
      end
      item
        Name = 'PITCH_MAX'
        ParamType = ptInput
      end
      item
        Name = 'ROLL_MAX'
        ParamType = ptInput
      end
      item
        Name = 'HEIGHT_MIN'
        ParamType = ptInput
      end
      item
        Name = 'ALTITUDE_MAX'
        ParamType = ptInput
      end
      item
        Name = 'MARCHE'
        ParamType = ptInput
      end>
  end
  object q_insert_alarm: TFDQuery
    Connection = PRINCIPALE.cn1
    SQL.Strings = (
      'INSERT INTO ETL.BOX_ALARM ('
      '    MARCHE,'
      '    GTOT_MIN,'
      '    GTOT_MAX,'
      '    IAS_MAX,'
      '    VSI_MAX,'
      '    PITCH_MAX,'
      '    ROLL_MAX,'
      '    HEIGHT_MIN,'
      '    ALTITUDE_MAX'
      ') VALUES ('
      '    :marche,'
      '    :gtot_min,'
      '    :gtot_max,'
      '    :ias_max,'
      '    :vsi_max,'
      '    :pitch_max,'
      '    :roll_max,'
      '    :height_min,'
      '    :altitude_max'
      ');'
      '')
    Left = 267
    Top = 202
    ParamData = <
      item
        Name = 'MARCHE'
        ParamType = ptInput
      end
      item
        Name = 'GTOT_MIN'
        ParamType = ptInput
      end
      item
        Name = 'GTOT_MAX'
        ParamType = ptInput
      end
      item
        Name = 'IAS_MAX'
        ParamType = ptInput
      end
      item
        Name = 'VSI_MAX'
        ParamType = ptInput
      end
      item
        Name = 'PITCH_MAX'
        ParamType = ptInput
      end
      item
        Name = 'ROLL_MAX'
        ParamType = ptInput
      end
      item
        Name = 'HEIGHT_MIN'
        ParamType = ptInput
      end
      item
        Name = 'ALTITUDE_MAX'
        ParamType = ptInput
      end>
  end
